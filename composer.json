{
    "name": "tubssz/doctrine-dateinterval",
    "description": "Supports DateInterval in Doctrine DBAL and ORM.",
    "keywords": ["doctrine", "date", "interval"],
    "homepage": "https://github.com/tubssz/php-doctrine-dateinterval",
    "license": "MIT",
    "authors": [
        {
            "name": "Kevin Herrera",
            "email": "kevin@herrera.io",
            "homepage": "http://kevin.herrera.io"
        },
        {
            "name": "Andreas Lemke",
            "email": "andreas.lemke@tu-braunschweig.de"
        }
    ],
    "support": {
        "issues": "https://github.com/tubssz/php-doctrine-dateinterval/issues"
    },
    "require": {
        "php": ">=5.3.3|7",
        "doctrine/dbal": "~2,>=2.2.3",
        "tubssz/date-interval": "~1.3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*|~4|~5",
        "doctrine/orm": "~2,>=2.2.3"
    },
    "autoload": {
        "psr-0": {
            "Herrera\\Doctrine": "src/lib"
        }
    },
    "config": {
        "bin-dir": "bin",
        "vendor-dir": "src/vendors"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
